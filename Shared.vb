        Public Shared Sub MyDeCompileLine(My_InputCodeLine As String) ' Converts each line of source code into a FlowChart (and symbol if required)
            Dim SymbolName As String
            Dim PathName As String
            Dim MyErrors As Int32
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 54 Then MyTracing("@MyDeCompileLine")

            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MSG_MyTrace1(1050, "Shared:  My De Compile Line")
            End If
            If Len(My_InputCodeLine) < 1 Then Return
            If Left(MyTrim(My_InputCodeLine), 1) = "/" Then
                If Left(My_InputCodeLine, 2) <> "//" Then   'For C code lines starting with a comment
                    ImportLineOrFile(My_InputCodeLine)
                    Exit Sub
                End If
            End If

            DisplayMyStatus("Line Number " & MyUniverse.MySS.Inputs.LineNumberIn.ToString)
            PathName = "??"

            MyUniverse.MySS.Inputs.KeyLineOrFile = MyTrim(My_InputCodeLine)
            MyErrors = 1000000 'number of lines of code or errors '9,223,372,036,854,775,807 ' This should be enough to avoid a forever loop
            MyReSort("Named", Named_FileSintax, Named_FileSintax_Isam, TopOfFile("named"))
            While MyErrors > 0  ' will exit when the end of file is reached.
                MyErrors -= 1
