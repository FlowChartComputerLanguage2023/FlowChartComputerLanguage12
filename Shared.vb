        Friend Shared Sub ShowSorts(MyTable As String, Total As Int32) 'Displays the number of sorts made
            Dim Count As Int32
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 53 Then MyTracing("@ShowSorts " & Total.ToString)
            Count = MyUniverse.MyCheatSheet.BugsCounted
            MyUniverse.MyCheatSheet.BugsCounted = 0
            If Total > 0 And Count > 0 Then
                DisplayMyStatus(MyTable & " Still working ... sorted " & Total & " Total issues's detected " & Count)
            ElseIf Count > 0 Then
                DisplayMyStatus(MyTable & " Still working ... issues's detected " & Count)
            ElseIf Total > 0 Then
                DisplayMyStatus(MyTable & " Still working ... sorted " & Total)
                'else no change in the display
            ElseIf Total = 0 And Count = 0 Then
                MyTracing(".")
            Else
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 77 Then
                    MSG_MyTrace3(1001, "Shared:  ShowSorts " & MyTable & "  " & Total.ToString)
                End If
            End If
        End Sub

