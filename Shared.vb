        Public Shared Function CreateFileNameFromSintax(SinTaxLine As String) As String
            Dim X, Y As String
            Dim I As Int32
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 56 Then
                MSG_MyTrace1(1681, "Shared:  Create File Name From Syntax --> " + SinTaxLine)
            End If
            Y = ""
            X = SinTaxLine
            CreateFileNameFromSintax = ""
            While Len(X) > 0
                Select Case LCase(IsThisAWhat(X))
                    Case MyUniverse.SysGen.Constants.SyntaxFieldDelimiters
                        X = Mid(X, Len(FD) + 1, Len(X))
                    Case MyUniverse.ProgramOptions.C_L_MultiLine
                        Pop1(X, FD)
                        CreateFileNameFromSintax &= "a"
                    Case MyUniverse.SysGen.Constants.SyntaxCameFromLine
                        CreateFileNameFromSintax &= "g"
                        Pop1(X, FD)
                    Case MyUniverse.SysGen.Constants.SyntaxGotoNextLine
                        CreateFileNameFromSintax &= "c"
                        Pop1(X, FD)
                    Case MyUniverse.ProgramOptions.C_L_Comment
                        CreateFileNameFromSintax &= "r"
                        Pop1(X, FD)
                    Case MyUniverse.ProgramOptions.C_L_Extensions
                        Y = Pop(X, FD) ' Save the extension till last
                        CreateFileNameFromSintax &= "e"
                    Case MyUniverse.ProgramOptions.C_L_VarChars
                        CreateFileNameFromSintax &= Pop1(X, FD)
                    Case MyUniverse.SysGen.Constants.SyntaxComments 'Ignore the rest of the line
                        CreateFileNameFromSintax &= "h"
                        PopLine(X)
                    Case MyUniverse.SysGen.Constants.SyntaxQuotes
                        'CreateFileNameFromSintax &=
                        PopQuote(X) 'Throw quote away???
                        CreateFileNameFromSintax &= "q"
                    Case MyUniverse.SysGen.Constants.SyntaxKeyWords
                        CreateFileNameFromSintax &= Pop(X, FD)'Save the actual keyword...
                    Case MyUniverse.SysGen.Constants.SyntaxFunctions
                        CreateFileNameFromSintax &= "f"
                        Pop(X, FD)
                    Case MyUniverse.SysGen.Constants.SyntaxOperators
                        CreateFileNameFromSintax &= "o"
                        Pop(X, FD)'Save the actual keyword...
                    Case MyUniverse.SysGen.Constants.SyntaxAlphas
                        CreateFileNameFromSintax &= "l"
                        Pop1(X, FD)'should be a variable name
                    Case MyUniverse.SysGen.Constants.SyntaxNumbers
                        Pop(X, FD) 'treated as a variable name
                        CreateFileNameFromSintax &= "3"
                    Case MyUniverse.SysGen.Constants.SyntaxSpecialCharacters
                        Pop(X, FD)
                        CreateFileNameFromSintax &= "_"
        'Case MyUniverse.SysGen.Constants.SyntaxVariables
        '    CreateFileNameFromSintax &= "V"
    '    Pop1(X, FD)
                    Case MyUniverse.SysGen.Constants.SyntaxUnknowns
                        CreateFileNameFromSintax &= "_"
                        Pop(X, FD)
                    Case Else
                        If IsThisAVariableName(No_FieldDelimiter(IsThisAWhat(X))) = True Then
                            CreateFileNameFromSintax &= "V"
                        Else
                            CreateFileNameFromSintax &= IsThisAVariableName(No_FieldDelimiter(IsThisAWhat(X)))
                        End If
                        Pop1(X, FD)
                End Select
            End While

            'remove any special characters from the file name
            For I = 1 To Len(CreateFileNameFromSintax)
                While IsThisASpecalCharacter(Mid(CreateFileNameFromSintax, I, 1)) And PrintAbleNull(Mid(CreateFileNameFromSintax, I, 1)) <> "_"
                    CreateFileNameFromSintax = Mid(CreateFileNameFromSintax, 1, I - 1) & Mid(CreateFileNameFromSintax, I + 1, Len(CreateFileNameFromSintax))
                    '2020 0907    mid(CreateFileNameFromSintax, I, 1) = "_" ' Over ride any left over special characters with an underscore
                End While
            Next
            If CreateFileNameFromSintax = "" Or MyIsNothing(CreateFileNameFromSintax) Then
                CreateFileNameFromSintax = MakeNewName("FileName_", SinTaxLine) & ".Symbol"
            Else
                'need to know if it will become a symbol or will become program code
                If Y = "" Then
                    CreateFileNameFromSintax &= ".symbol"
                Else
