        Public Shared Function BlockBreak(CodeLine As String, StartEndBlock As String) As String
            Dim mystart, myend, Temp As Int32
            Dim Debug1, Debug2 As String
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 55 Then MyTracing("@BlockBreak " & CodeLine)
            Temp = CInt(Len(StartEndBlock) / 2)
            Debug1 = Mid(StartEndBlock, 1, Temp)
            Debug2 = Mid(StartEndBlock, 1 + Temp, Temp)
            mystart = InStr(CodeLine, Mid(StartEndBlock, 1, Temp))
            myend = InStr(Mid(CodeLine, mystart + 1, Len(CodeLine)), Mid(StartEndBlock, Temp + 1, 1)) + +mystart
            If mystart <> 0 And myend <> 0 And myend > mystart Then
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 45 Then MyTrace(2153, Mid(CodeLine, mystart, myend + 2))
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 45 Then MyTracing("--BlockBreak " & Mid(CodeLine, mystart, myend + 2))
                Return Mid(CodeLine, mystart + 1, myend - mystart)
            End If
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 55 Then MyTracing("-BlockBreak ")
            Return ""
        End Function

        Public Shared Function Block(CodeLine As String) As String
            Dim RTN As String
            Dim X As String = Chr(34)
            RTN = BlockBreak(CodeLine, "'<>'")
            If RTN = "" Then RTN = BlockBreak(CodeLine, "''")
            If RTN = "" Then RTN = BlockBreak(CodeLine, X & X)
            If RTN = "" Then RTN = BlockBreak(CodeLine, "()")
            If RTN = "" Then RTN = BlockBreak(CodeLine, "{}")
            If RTN = "" Then RTN = BlockBreak(CodeLine, "[]")
            If RTN = "" Then RTN = BlockBreak(CodeLine, "<>")
            If RTN = "" Then
                Return CodeLine
            Else
                Return Block(RTN)
            End If
        End Function

        Public Shared Function IsThisAHexDigit(A As String) As Boolean
            Dim I As Int32
            For I = 1 To Len(A)
                Select Case Mid(A, I, 1)
                    Case "1", "2", "3", "4", "5", "6", "7", "8", "9", "0", "a", "b", "c", "d", "e", "f"
                    Case Else
                        Return False
                End Select
            Next I
            Return True
        End Function



        Public Shared Function Hex2Bin(HEX0x0000 As String) As Int32
            Dim A As String
            A = MyTrim(HEX0x0000)
            A = TrimOff(A, "'")
            'todo this should also have an error message
            If A = "0x" Then Return &HFFFFF 'todo should not return everything
            If Left(A, 2) = "0x" Then
                A = Mid(A, 3)
            Else
                MSG_Abug(2343, "Invalid hex ", HEX0x0000, "")
                Return -1
            End If
            A = Trim(A)
            If IsThisAHexDigit(A) = True Then Return Convert.ToInt32(A, 16)
            MSG_Abug(2344, "@ HEX2BIN can not convert ", A, HEX0x0000)
            Return -1
        End Function


        Public Shared Function TrimOff(A As String, Remove As String) As String
            TrimOff = A
            While Left(TrimOff, 1) = Remove
                TrimOff = Mid(TrimOff, 2)
            End While
