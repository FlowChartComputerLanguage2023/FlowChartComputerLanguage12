        Public Shared Sub FixSinTaxCode(IndexNamed As Int32)
            Dim IndexSymbol As Int32
            Dim PointName As String
            Dim Debug1, debug2 As String
            MyTracing("@FixSyntaxCode")
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MSG_MyTrace1(1882, "Shared:  Fix Syntax Code()")
            End If
            IndexSymbol = Named_TableSymbolIndexes(IndexNamed)
            If Symbol_TableCoded_String(IndexSymbol) <> "/name" Then Exit Sub
            IndexSymbol += 1
            While Symbol_TableCoded_String(IndexSymbol) <> "/name" And IndexSymbol <= TopOfFile("Symbol")
                Select Case Symbol_TableCoded_String(IndexSymbol)
                    Case "/point"
                        PointName = NameOfPointOnly(Symbol_Table_NameOfPoint(IndexSymbol))
                        If Len(PointName) < 1 Then
                            If Len(SymbolWindow.ComboBoxPointNameList.Text) = 0 Then
                                MSG_Abug(1883, "Point name Is Nothing changed To unknown", MyShowNamedRecords(True, IndexNamed), MyShowSymbolGraphic(True, IndexSymbol))
                                Symbol_Table_NameOfPoint(IndexSymbol, MakeNewName("Var_", "Var_"))
                            Else
                                'MSG_Abug(1884, "Point name Is Nothing changed To " & SymbolWindow.ComboBoxPointNameList.text, MyShowNamedRecords(true, IndexNamed), MyShowSymbolGraphic(true, IndexSymbol))
                                If Len(SymbolWindow.ComboBoxPointNameList.Text) > 0 Then
                                    Symbol_Table_NameOfPoint(IndexSymbol, SymbolWindow.ComboBoxPointNameList.Text)
                                    SymbolWindow.ComboBoxPointNameList.Text = "" 'Used this name so erase it
                                Else
                                    Symbol_Table_NameOfPoint(IndexSymbol, "unknown2")
                                End If
                            End If
                        Else
                            'todo this is failing to find what it should find
                            Debug1 = Named_TableSintax(IndexNamed)
                            debug2 = MyUniverse.SysGen.Constants.RMStart & PointName & "."
                            MyTrace(2291, "syntax " & vbCrLf & Debug1 & vbCrLf & debug2 & vbCrLf)
                            If InStr(Named_TableSintax(IndexNamed), MyUniverse.SysGen.Constants.RMStart & PointName & ".") = 0 Then
                                'todo need to tell what and where this was not found, and why
                                MSG_Abug(1885, "this is failing to find what it should find",
"point name = " & MyUniverse.SysGen.Constants.RMStart & PointName & ".", "Syntax = " & Named_TableSintax(IndexNamed))
                                'Todo add the name to the code/syntax/pointnames list
                                MyTrace(2292, MyUniverse.SysGen.Constants.RMStart & Symbol_Table_NameOfPoint(IndexSymbol) & "." & "  is not in the micro text " & MyShowSymbolPoint(False, IndexSymbol) & vbCrLf & Named_TableSintax(IndexNamed) & vbCrLf)
                                If Left(Symbol_Table_NameOfPoint(IndexSymbol), Len(MyUniverse.SysGen.Constants.RMStart)) = MyUniverse.SysGen.Constants.RMStart Then
                                    MSG_Abug(1887, "Not sure how I can get here", MyShowSymbolGraphic(True, IndexSymbol), Symbol_Table_NameOfPoint(IndexSymbol))
                                Else
                                    Named_TableSintax(IndexNamed, Named_TableSintax(IndexNamed) & vbCrLf & AddRM(Symbol_Table_NameOfPoint(IndexSymbol) & ".PATHName"))
                                    AddPointNameToDropDownX(Symbol_Table_NameOfPoint(IndexSymbol))
                                    SymbolWindow.TextBoxNamedProgramText.Text = Named_TableSintax(IndexNamed)
                                    AddPointNameToDropDownX(Symbol_Table_NameOfPoint(IndexSymbol) + FD + MyShowSymbolRecords(True, IndexSymbol))
                                End If
                            Else
                                'todo make sure that the point name is in the drop down list on the symbol screen
                                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 55 Then MyTrace(2293, "point found in syntax point name = " & AddRM(PointName & "."))
                                'The point name is found in the micro code correctly (almost) formated
                                If CheckPointNameToDropDown(PointName) <> 0 Then
                                    MSG_Abug(1888, "The point name " & PointName & " is not in the drop down for ", MyShowNamedRecords(False, IndexNamed), "")
                                End If
                            End If
                        End If
                    Case "/line"
                        If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 92 Then
                            MyInfo(794, "Not a point to fix syntax ", MyShowSymbolGraphic(True, IndexSymbol), "") 'todo need to change this bug number
                        End If
                    Case Else 'not a point to fix
                        If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 20 Then
                            MyInfo(794, "Not a point ", MyShowSymbolGraphic(True, IndexSymbol), "") 'todo need to change this bug number
                        End If
                End Select
                IndexSymbol += 1
            End While
        End Sub


        Public Shared Function FindingMyBugsNets() As Int32
            Dim Idex, Jdex, K As Int32
            Dim TempI, tempJ As String
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MSG_MyTrace1(1889, "Shared:  Finding My Bugs: Nets()")
            End If
            For Idex = LBound(Net_FileLinks) + 1 To UBound(Net_FileLinks)
                Select Case LCase(FlowChart_TableCode(Idex))
                    Case My_keyWord(My_KeyConstUse)
                    Case "error"
                    Case MyUniverse.SysGen.Constants.SyntaxUnknowns
                    Case "/path" ' We are only interested in the paths (for now, )
                        Jdex = FindIndex_In_TableNetLinks(Idex) ' make sure that path is in a net
                        If Jdex = constantMyErrorCode Then
                            MSG_Abug(1890, "Finding My Bugs: Path not in any path link", MyShowFlowChartRecord(True, Idex), "")
                        Else
                            If FlowChart_TablePath_Name(Idex) <> Net_TableNames(FindIndex_In_TableNetLinks(Idex)) Then
                                MSG_Abug(1891, "Finding My Bugs: Path name does not match path links name", MyShowNetLnks(Idex), MyShowNetLnks(Jdex))
                            End If
                        End If
                        ' A I N F O 2 (183)
                        TempI = Net_FileLinks(Idex)
                        For Jdex = Idex + 1 To UBound(Net_FileLinks)
                            ' A I N F O 2 (184)
                            If Len(Net_TableNames(Idex)) > 0 Then
                                If Net_TableNames(Idex) = Net_TableNames(Jdex) Then
                                    ' A I N F O 2 (172)
                                    Return MSG_Abug(1892, "Finding My Bugs:  2 different paths with the same name.", MyShowNetLnks(Idex), MyShowNetLnks(Jdex))
                                End If
                            End If
                            ' A I N F O 2 (185)
                            tempJ = Net_FileLinks(Jdex)
                            ' A I N F O 2 (186)
                            While Len(tempJ) > 0
                                ' A I N F O 2 (187)
                                K = PopValue(tempJ)
                                'search for ,x, in temp1
                                If InStr(FD & K.ToString & FD, TempI) <> 0 Then
                                    ' A I N F O 2 (173)
                                    Return MSG_Abug(1893, "Finding My Bugs: 2 path links have the same path", MyShowNetLnks(Idex), MyShowNetLnks(Jdex))
                                End If
                            End While
                            ' A I N F O 2 (188)
                        Next
                End Select
                ' A I N F O 2 (189)
            Next
            ' A I N F O 2 (174)
            Return 0
        End Function




