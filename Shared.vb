        Public Shared Sub DoOption(WhichOne As Int32, HowMuch As String, OtherOptions As String)
            Dim I As Int32
            Dim X As String
            Dim Level As Int32

            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 17 Then
                MSG_MyTrace1(1333, "Shared:  Do Option() " & WhichOne.ToString & vbTab & HowMuch & vbTab & OtherOptions)
            End If
            Select Case WhichOne
                Case 51
                    For I = LBound(MyMessageBits) To UBound(MyMessageBits)
                        MyMessageBits(I) = 255 ' Turn "on" all message and debug bits
                    Next I
                    For I = LBound(MyMessageBits1) To UBound(MyMessageBits1)
                        MyMessageBits1(I) = 255 ' Turn "on" all message and debug bits
                    Next I


                Case 53 '    Turn off information messages
                    For I = 0 To OptionsWindow.ComboBoxDebug.Items.Count - 1
                        X = OptionsWindow.ComboBoxDebug.Items.Item(I).ToString
                        Level = PopValue(X)
                        If LCase(Trim(Pop(X, MyUniverse.SysGen.Constants.ConstantDelimeters))) = "information" Then
                            BitSet(Level, "off") ' turn on or off all information that can show up
                        End If
                    Next
                Case 54 '    Turn off warning messages
                    For I = 0 To OptionsWindow.ComboBoxDebug.Items.Count - 1
                        X = OptionsWindow.ComboBoxDebug.Items.Item(I).ToString
                        Level = PopValue(X)
                        If LCase(Trim(Pop(X, MyUniverse.SysGen.Constants.ConstantDelimeters))) = "warning" Then
                            BitSet(Level, "off") ' turn on or off all information that can show up
                        End If
                    Next
                Case 55 '    Turn off wrong messages
                    For I = 0 To OptionsWindow.ComboBoxDebug.Items.Count - 1
                        X = OptionsWindow.ComboBoxDebug.Items.Item(I).ToString
                        Level = PopValue(X)
                        If LCase(Trim(Pop(X, MyUniverse.SysGen.Constants.ConstantDelimeters))) = "wrong" Then
                            BitSet(Level, "off") ' turn on or off all information that can show up
                        End If
                    Next
                Case Else
                    If WhichOne >= 998 And WhichOne < 10000 Then
                        BitSet(WhichOne, HowMuch)
                    End If
            End Select
        End Sub


