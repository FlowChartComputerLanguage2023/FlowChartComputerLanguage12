        Public Shared Sub MyPlacement(AddOrNot As Boolean)
            Dim x, y, s, maxXY1 As Int32
            Const PlaceMent As Int32 = 16 'times the size of the symbol (/2)
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 51 Then
                MSG_MyTrace1(1093, "Shared:  My Placement()")
            End If
            MyUniverse.SysGen.Snaps.MaxSymbolInYSpacing = MyUniverse.SysGen.Constants.constantSymbolCenter * MyUniverse.SysGen.Constants.constantSpacingFactor
            x = MyUniverse.SysGen.Placement.Xx
            y = MyUniverse.SysGen.Placement.Yy
            s = MyUniverse.SysGen.Constants.constantSymbolCenter
            maxXY1 = MyUniverse.SysGen.Snaps.MaxSymbolInYSpacing * PlaceMent
            If AddOrNot = True Then
                y += s * PlaceMent
            End If
            If y > maxXY1 Then
                x += s * PlaceMent ' Move over 
                y = s
            End If
            If x < s * PlaceMent Then x = s * PlaceMent
            If y < s * 2 Then y = s * PlaceMent
            MyUniverse.SysGen.Placement.Xx = x
            MyUniverse.SysGen.Placement.Yy = y
            'Pan(x, y) 'pan this xy to the center of the screen
        End Sub


