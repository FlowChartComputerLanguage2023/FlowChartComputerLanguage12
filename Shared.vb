        Public Shared Function ROUTERCheckLinesEndAtEachOther(XY1 As MyLineStructure, XY2 As MyLineStructure) As MyPointStructure
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MyTracing("@ROUTERCheckLinesEndAtEachOther")
            End If
            If XY1.a.Xx = XY2.a.Xx Then If XY1.a.Yy = XY2.a.Yy Then Return MyPoint1XY(XY1.a.Xx, XY1.a.Yy)
            If XY1.a.Xx = XY2.b.Xx Then If XY1.a.Yy = XY2.b.Yy Then Return MyPoint1XY(XY1.a.Xx, XY1.a.Yy)
            If XY1.b.Xx = XY2.a.Xx Then If XY1.b.Yy = XY2.a.Yy Then Return MyPoint1XY(XY1.b.Xx, XY1.b.Yy)
            If XY1.b.Xx = XY2.b.Xx Then If XY1.b.Yy = XY2.b.Yy Then Return MyPoint1XY(XY1.b.Xx, XY1.b.Yy)
            Return MyUniverse.MyStaticData.ZeroZero
        End Function


        'search backwards until you are below ?
        Public Shared Function FindLowerXY(Idex As Int32, MyISam() As Int32, MyArray() As Int32, ErrorMessage As String, ValueTofindBelow As Int32) As Int32
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 45 Then MyTracing("@FindLowerXY")
            If Idex >= 1 Then
                FindLowerXY = MyISam(Idex - 1) ' We are searching for one lower then the current selected index
            Else
                FindLowerXY = UBound(MyISam)
            End If

            Do
                While InvalidIndex(FindLowerXY, FlowChart_FilePathSymbolName) = False And
    FlowChart_TableCode(FindLowerXY) <> "/path"
                    'make sure that the next on is not this one.
                    If Idex <> MyISam(FindLowerXY - 1) Then
                        FindLowerXY = MyISam(FindLowerXY - 1) ' back up to the path before this on.
                    Else
                        If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 61 Then MSG_Ainfo1(2073, " Should we jump over this one and try again?", Idex.ToString, MyISam(FindLowerXY - 1).ToString)
                        Return FindIndex1(MyISam)
                    End If
                End While
                If FindLowerXY < 1 Then
                    Return FindIndex1(MyISam)
                End If
                FindLowerXY = MyCompared2(MyArray, MyISam, FindLowerXY, Idex)
                Select Case MyCompared2(MyArray, MyISam, FindLowerXY, Idex)
                    Case -10, -9, -8, -7, -6 'hack this is never turned
                        MSG_Abug(2074, "Finding My Bugs: " & MyCompared2(MyArray, MyISam, FindLowerXY, Idex) & ErrorMessage, MyShowFlowChartRecord(True, FindLowerXY), MyShowFlowChartRecord(True, Idex))
                    Case -5 ' usually an invalid index, but coube many things.(Or invalid pointer)
                        If FindLowerXY < 1 Then Return FindIndex1(MyISam)
                        Return FindLowerXY
                    Case -4 'hack this is never turned
                        MSG_Abug(2075, "Finding My Bugs: " & MyCompared2(MyArray, MyISam, FindLowerXY, Idex) & ErrorMessage, MyShowFlowChartRecord(True, FindLowerXY), MyShowFlowChartRecord(True, Idex))
                        MyCompared2(MyArray, MyISam, FindLowerXY, Idex)
                    Case -3 'A is higher than B  and C = nothing
                        MSG_Abug(2076, "Finding My Bugs: " & MyCompared2(MyArray, MyISam, FindLowerXY, Idex) & ErrorMessage, MyShowFlowChartRecord(True, FindLowerXY), MyShowFlowChartRecord(True, Idex))
                        MyCompared2(MyArray, MyISam, FindLowerXY, Idex)
                    Case -2
                        MSG_Abug(2077, "Finding My Bugs: " & ErrorMessage, MyCompared2(MyArray, MyISam, FindLowerXY, Idex) & MyShowFlowChartRecord(True, FindLowerXY), MyShowFlowChartRecord(True, Idex))
                        MyCompared2(MyArray, MyISam, FindLowerXY, Idex)
                    Case -1 'FindLowerXY.x1 is less than index.x1
                        Return FindLowerXY
                    Case 0
                        'the two are equal
                        If InvalidIndex(FindLowerXY, FlowChart_FilePathSymbolName) = True Then 'Is FindLowerXY valid
                            Return FindIndex1(MyISam) 'hack (who knows where the lowest /path is?
                        Else
                            If InvalidIndex(MyISam(FindLowerXY - 1), FlowChart_FilePathSymbolName) = True Then
                                Return FindLowerXY
                            Else
                                FindLowerXY = MyISam(FindLowerXY - 1)
                            End If
                        End If
                    Case 1
                        If InvalidIndex(FindLowerXY, FlowChart_FilePathSymbolName) Then Return FindIndex1(MyISam) 'fail safe hack
                        If InvalidIndex(MyISam(FindLowerXY), FlowChart_FilePathSymbolName) = True Then Return FindLowerXY 'If we can go no farther back then this is it.
                        'So lets try farther back down the list
                        If InvalidIndex(MyISam(FindLowerXY - 1), FlowChart_FilePathSymbolName) = True Then Return FindLowerXY
                        FindLowerXY = MyISam(FindLowerXY - 1) ' Greater so back up more (OK, This should never happend because the index for the flow chart should have been pointing to something that is aleast equal
                    Case 2
                        MSG_Abug(2078, "Finding My Bugs: " & ErrorMessage, MyShowFlowChartRecord(True, FindLowerXY), MyShowFlowChartRecord(True, Idex))
                        MyCompared2(MyArray, MyISam, FindLowerXY, Idex)
                    Case 3 'hack this is never turned
                        MSG_Abug(2079, "Finding My Bugs: " & ErrorMessage, MyShowFlowChartRecord(True, FindLowerXY), MyShowFlowChartRecord(True, Idex))
                        MyCompared2(MyArray, MyISam, FindLowerXY, Idex)
                    Case 4 'hack this is never turned
                        MSG_Abug(2080, "Finding My Bugs: " & ErrorMessage, MyShowFlowChartRecord(True, FindLowerXY), MyShowFlowChartRecord(True, Idex))
                        MyCompared2(MyArray, MyISam, FindLowerXY, Idex)
                    Case 5 'hack this is never turned
                        MSG_Abug(2081, "Finding My Bugs: " & ErrorMessage, MyShowFlowChartRecord(True, FindLowerXY), MyShowFlowChartRecord(True, Idex))
                        MyCompared2(MyArray, MyISam, FindLowerXY, Idex)
                    Case 6, 7, 8, 9, 10 'hack this is never turned
                        MSG_Abug(2082, "Finding My Bugs: " & ErrorMessage, MyShowFlowChartRecord(True, FindLowerXY), MyShowFlowChartRecord(True, Idex))
                End Select
            Loop While 1 = 1 'forever loop
        End Function



