            B = A
            If MyIsNothing(MyUniverse.SysGen.Constants.RMStart) = True Then Return A
            B = B.Replace(MyUniverse.SysGen.Constants.RMStart, Start)
            B = B.Replace(MyUniverse.SysGen.Constants.RMEnd, Ending)
            Return B
        End Function
