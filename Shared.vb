        Public Shared Function MakeStatementReplacements(CodeLine As String) As String
            Dim Idex As Int32
            Dim A As String
            Dim myarray() As String
            ReDim myarray(256)
            MyTracing("@MakeStateMentReplacements")
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 56 Then
                MSG_MyTrace1(1659, "Shared:  Make Statement Replacements() " + CodeLine)
            End If
            MakeStatementReplacements = ""
            ' Idex might be counting twice and missing Syntax
            Idex = 1
            MyUniverse.MyStaticData.Path_Names = MyParse(myarray, CodeLine)
            MyUniverse.MyStaticData.Path_NamesCounted = CountPathNames(MyUniverse.MyStaticData.Path_Names)
            'A = MyUniverse.MyStaticData.PathNames
            While PrintAbleNull(myarray(Idex)) <> "_"
                A = myarray(Idex)
                Select Case LCase(No_FieldDelimiter(IsThisAWhat(A)))
                    Case MyUniverse.ProgramOptions.C_L_MultiLine
                    Case MyUniverse.ProgramOptions.C_L_Comment
                    Case MyUniverse.ProgramOptions.C_L_Extensions
                    Case MyUniverse.ProgramOptions.C_L_VarChars
                    Case MyUniverse.SysGen.Constants.SyntaxCameFromLine
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_Comment & A)
                    Case MyUniverse.SysGen.Constants.SyntaxGotoNextLine
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & MyUniverse.ProgramOptions.C_L_Comment & A)
                    Case MyUniverse.SysGen.Constants.SyntaxComments 'Ignore the rest of the line
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & MyUniverse.ProgramOptions.C_L_Comment & A)' Just add a comment
                    Case MyUniverse.SysGen.Constants.SyntaxQuotes
                        ' Save The First Quote
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & A) ' This should be Quote Marks
                    Case MyUniverse.SysGen.Constants.SyntaxKeyWords, MyUniverse.SysGen.Constants.SyntaxFunctions, MyUniverse.SysGen.Constants.SyntaxOperators
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & A) & MyUniverse.ProgramOptions.C_L_WhiteSpace' put space arount any keyword
                    Case MyUniverse.SysGen.Constants.SyntaxAlphas
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & AddRM(A & ".value"))
                    Case MyUniverse.SysGen.Constants.SyntaxNumbers
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & AddRM(A & ".value")) & MyUniverse.ProgramOptions.C_L_WhiteSpace
                    Case MyUniverse.SysGen.Constants.SyntaxSpecialCharacters
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & A) & MyUniverse.ProgramOptions.C_L_WhiteSpace
        'Case MyUniverse.SysGen.Constants.SyntaxVariables
        'MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & AddRM(A & ".name")) & MyUniverse.ProgramOptions.C_L_WhiteSpace
                    Case MyUniverse.SysGen.Constants.SyntaxUnknowns
                        MSG_Abug(1660, "unknown Character Classification", A, IsThisAWhat(A))
                        MakeStatementReplacements = Trim(MakeStatementReplacements & A)
                    Case Else
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & AddRM(A & ".name")) & MyUniverse.ProgramOptions.C_L_WhiteSpace
                        MSG_Abug(1661, "program issue", "Did Not take care Of a IsThisAWhat", IsThisAWhat(A))
                        MakeStatementReplacements = Trim(MakeStatementReplacements & MyUniverse.ProgramOptions.C_L_WhiteSpace & A) & MyUniverse.ProgramOptions.C_L_WhiteSpace
                End Select
                ' I need to explain why I did this and am missing a null field?
                Idex += 1
            End While
            If Right(MakeStatementReplacements, Len(FD)) <> FD Then
                MakeStatementReplacements &= FD
            End If
        End Function



        Public Shared Function MakeStatementSintax(MyArray() As String) As String
            Dim Idex As Int32
            Dim A1, B1 As String
            Dim Delim As String
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 52 Then MyTracing("@MakeStatementSyntax")

            MakeStatementSintax = ""
            ' Idex might be counting twice and missing Syntax
            Delim = "" ' Do not start with a comma(or what ever the field delimeter is right now
            Idex = 1
            While PrintAbleNull(MyArray(Idex)) <> "_"
                A1 = MyArray(Idex)
                B1 = LCase(IsThisAWhat(A1))
                Select Case B1
                    Case MyUniverse.ProgramOptions.C_L_MultiLine
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.ProgramOptions.C_L_MultiLine)
                    Case MyUniverse.SysGen.Constants.SyntaxGotoNextLine
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxGotoNextLine & FD & MakePathName(MyArray(Idex + 1))) 'HACK making it a code keyword GOTO, hope the computer language has that as a keyword!!!!!!!
                        Idex += 1
                    Case MyUniverse.SysGen.Constants.SyntaxCameFromLine
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxCameFromLine & FD & MakePathName(MyArray(Idex + 1)) & ":")
                        Idex += 1
                    Case MyUniverse.ProgramOptions.C_L_Comment
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.ProgramOptions.C_L_Comment)
                    Case MyUniverse.ProgramOptions.C_L_Extensions
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & ThisExtension(MyUniverse.ProgramOptions.C_L_Extension, 1))
                    Case MyUniverse.ProgramOptions.C_L_VarChars 'todo Not so sure about this!!!!!!! ' Possible error
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxGotoNextLine)
                    Case MyUniverse.SysGen.Constants.SyntaxComments 'Ignore the rest of the line
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxComments)
                    Case MyUniverse.SysGen.Constants.SyntaxQuotes
                        ' Save The First Quote
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxQuotes)
                    Case MyUniverse.SysGen.Constants.SyntaxKeyWords
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & A1)
                    Case MyUniverse.SysGen.Constants.SyntaxOperators
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxOperators)
                    Case MyUniverse.SysGen.Constants.SyntaxFunctions
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxFunctions)
                    Case MyUniverse.SysGen.Constants.SyntaxSpecialCharacters
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & B1)
                    Case MyUniverse.SysGen.Constants.SyntaxNumbers
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxNumbers)
        ''''''''Case MyUniverse.SysGen.Constants.SyntaxVariables
        ''''''''MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxVariables)
                    Case MyUniverse.SysGen.Constants.SyntaxAlphas
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxAlphas)
                    Case MyUniverse.SysGen.Constants.SyntaxAlphaNumberic
                        'todo THis needs to be changed to be {[(VariableName.PATH)]}
                        'MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxVariables)
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MakePathName(A1))
                    Case MyUniverse.SysGen.Constants.SyntaxFieldDelimiters 'donothing
                        MakeStatementSintax = Trim(MakeStatementSintax)
                    Case MyUniverse.SysGen.Constants.SyntaxUnknowns
                        MSG_Abug(1662, "unknown Character Clasifacition", A1 & "=" & IsThisAWhat(A1), MyUniverse.SysGen.Constants.SyntaxUnknowns)
                        MakeStatementSintax = Trim(MakeStatementSintax & Delim & MyUniverse.SysGen.Constants.SyntaxUnknowns & A1)
                    Case Else ' This is a variable period pointAttribute
                        If InStr(A1, ".") <> 0 Then
                            If InStr(A1, MyUniverse.SysGen.Constants.RMStart) > 0 Then
                                If InStr(A1, MyUniverse.SysGen.Constants.RMEnd) > 0 Then
                                    MakeStatementSintax = Trim(MakeStatementSintax & Delim & MakePathName(A1))
                                Else
                                    MSG_Abug(1663, "Variable is missing the End marker " & MyUniverse.SysGen.Constants.RMEnd, " But has the first marker " & MyUniverse.SysGen.Constants.RMStart, " and has the period")
                                End If
                            Else
                                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 51 Then MSG_Ainfo(1664, "Variable is missing the Start marker " & MyUniverse.SysGen.Constants.RMEnd, A1, " and has the period - assumed it is a literial with out quotes")
                                MakeStatementSintax = Trim(MakeStatementSintax & Delim & A1)
                            End If
                        Else
                            MSG_Abug(1665, "Unknow what this is", HighLight(A1), HighLight(IsThisAWhat(A1)))
                        End If
                End Select
                ' I need to explain why I did this and am missing a null field?
                'While Right(MakeStatementSintax, 2) = Delim & Delim
                ' MakeStatementSintax = Left(MakeStatementSintax, Len(MakeStatementSintax) - 1)
                ' End While
                ' While Left(MakeStatementSintax, 1) = Delim
                ' MakeStatementSintax = Mid(MakeStatementSintax, 2, Len(MakeStatementSintax))
                ' End While
                Idex += 1
                Delim = FD ' Now make everthing seperated witha Field delimeter
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 67 Then
                    MyTrace(1666, "Shared:  Make Statement Syntax() " & vbCrLf & vbTab & MakeStatementSintax)
                End If
            End While
            If Right(MakeStatementSintax, Len(FD)) <> FD Then
                MakeStatementSintax &= FD
            End If
            DisplayMyStatus("Syntax made " & MakeStatementSintax)
            'Check that the symbol matches the sintax, matches the point names, matches the rest
        End Function


