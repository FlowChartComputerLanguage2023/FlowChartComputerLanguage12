        End Sub


        Public Shared Function FindingMyBugs_ROUTER_IsLineOnLine(a As MyLineStructure, b As MyLineStructure) As Boolean
            Dim X As MyLineStructure = Nothing
            Dim Y As MyLineStructure = Nothing
            Dim DebugTemp1, dEBUGtEMP2 As String
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MSG_MyTrace1(2101, "Shared: Finding My Bugs: Router s Line On Line() " + MyShowLineXYXY(a) + MyShowLineXYXY(b))
            End If
            If MyOptionTest(31) = False Then Return True
            'order them 
            X.a.Xx = MyMin(a.a.Xx, a.b.Xx)
            X.b.Xx = MyMax(a.a.Xx, a.b.Xx)
            X.a.Yy = MyMin(a.a.Yy, a.b.Yy)
            X.b.Yy = MyMax(a.a.Yy, a.b.Yy)


            Y.a.Xx = MyMin(b.a.Xx, b.b.Xx)
            Y.b.Xx = MyMax(b.a.Xx, b.b.Xx)
            Y.a.Yy = MyMin(b.a.Yy, b.b.Yy)
            Y.b.Yy = MyMax(b.a.Yy, b.b.Yy)
            'does box/path a on box b
            DebugTemp1 = MyShow3Lines(X, Y, X)
            dEBUGtEMP2 = MyShow3Lines(a, b, a)
            'is A all leftof B
            If X.b.Xx < Y.a.Xx And X.b.Xx < Y.a.Xx Then Return False 'A is all left B
            'Is A all right of B
            If X.a.Xx > Y.b.Xx And X.a.Xx > Y.b.Xx Then Return False 'A is rightr of B

            'is A all above B
            If X.b.Yy < Y.b.Yy And X.b.Yy < Y.a.Yy Then Return False 'A is all left B
            'Is A all below B
            If X.a.Yy > Y.b.Yy And X.a.Yy > Y.b.Yy Then Return False 'A is rightr of B
            Return True

        End Function


        Public Shared Function ROUTER_Find_Lowest_Index(XY As Int32, MyArrayLong() As Int32, ISAM() As Int32) As Int32
            Dim Idex As Int32
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 45 Then MyTracing("@Router_Find_Lowest_Index")
            'need to make sure this returns that index, not the x
            Idex = FindIndexIniSAMTable("FlowChart", "DoNotAdd", MyArrayLong, ISAM, XY) '
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MSG_MyTrace1(2102, "Shared: ROUTER Find Lowest Index = " + CStr(XY))
            End If
            'gobackwards untill you are before it.
            While Idex > 1
                If XY = MyArrayLong(ISAM(Idex)) Then
                    Idex = ISAM(Idex - 1) 'one before this one.
                Else
                    Exit While
                End If
            End While
            Return Idex
        End Function

