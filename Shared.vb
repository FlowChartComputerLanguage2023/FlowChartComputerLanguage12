        'This is to add keywords from the grammer's
        Public Shared Sub AddGrammer()
            Dim I, J As Int32
            Dim S, T As String

            If MyUniverse.Grammer = True Then Return

            For I = 0 To UBound(Language_Grammer)
                S = Language_Grammer(I)
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(S)
                J = InStr(S, "::=")
                If J <> 0 Then
                    S = MyTrim(Mid(S, J + 3))
                    While Len(MyTrim(S)) > 0
                        T = MyTrim(PopGrammer(S))
                        '************** anything with a ' must also have a ending ' because its the end of the line
                        If Left(T, 1) = "'" Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing("This is a literal " & T)
                            ' check to see if this is a keyword?
                            If Len(T) = 8 And Left(T, 3) = "'0x" And Right(T, 1) = "'" And IsThisAHexDigit(Mid(T, 4, 4)) = True Then
                                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTrace(2347, "Single letter hex input " & T & Hex2Bin(TrimOff(T, "'")))
                            ElseIf Len(T) = 3 And Left(T, 1) = "'" And Right(T, 1) = "'" Then
                                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTrace(2347, "Single letter terminal " & T)
                            ElseIf IsThisA_KeyWord(Mid(T, 2, Len(T) - 2)) = True Then
                                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTrace(2347, "Already a keyword " & T)
                            ElseIf IsThisA_Operator(Mid(T, 2, Len(T) - 2)) = True Then
                                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTrace(2348, "Already an operator " & T)
                            ElseIf IsThisA_Function(Mid(T, 2, Len(T) - 2)) = True Then
                                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTrace(2349, "Already a function " & T)
                            ElseIf Len(T) = 5 And Mid(T, 3, 1) = "-" Then
                                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTrace(2350, "This literal is a character range " & T)
                            Else
                                Select Case MsgBox(T & vbCrLf & Language_Grammer(I), MsgBoxStyle.YesNo, "Should this be added as a keyword?")
                                    Case MsgBoxResult.Yes
                                        AddAtomsToKeywordOROperatorORFunctionList(AddRM("KeyWord"), Mid(T, 2, Len(T) - 2), Language_KeyWord)
                                    Case MsgBoxResult.No
                                End Select
                            End If

                            'this is OR must have one or the other. 
                        ElseIf T = "|" Then 'or then 
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing("This is OR'ed ")
                            'this is the end of the line, everyting after it is a comment that is removed
                        ElseIf T = ";" Then
                            S = "" 'ignore the rest of this line as a comment
                            ' this is a grouping 
                        ElseIf T = "(" Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(T)
                            ' end of the group
                        ElseIf T = ")" Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(T)

                            'this is optional group
                        ElseIf T = "{" Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(T)
                        ElseIf T = "}" Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(T)

                            'this is optional
                        ElseIf T = "[" Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(T)
                        ElseIf T = "]" Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(T)

                            'this is the start of the end of the line (Must be a quoted string literial
                        ElseIf T = "<" Then
                            MyTracing(T)
                        ElseIf T = ">" Then
                            MyTracing(T)

                        ElseIf T = "+" Then
                            MyTracing(T)
                        ElseIf T = "-" Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(T)
                        ElseIf T = "&" Then
                            MyTracing(T)
                        ElseIf T = "-" Or T = "+" Or T = "*" Then
                            MyTracing("BNF " & T)

                            'Check if this is a keyword, or operator (special kind of keyword used only in expressions? )
                        ElseIf FindMyKeyword(T) > 0 Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(T)
                        ElseIf FindMyOperator(T) > 0 Then
                            MyTracing(T)


                            'check if done
                        ElseIf T = "" Then
                            If S = "" Then
                                Exit While
                            End If

                            'check if this is a name for another rule
                        ElseIf FindMyGrammerName(T) <> "" Then
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTrace(2354, FindMyGrammerName("Grammer name " & T))
                        Else
                            'All else fails so this might be a problem
                            If IsThisA_Operator(T) Then MyTrace(2366, "Should this be added to keyword, function, operator or missing a grammer name?")
                            MSG_Abug(2356, "Name in /grammer is not defined ", T, vbCrLf & S & vbCrLf & Language_Grammer(I))
                        End If
                    End While
                Else
                    If S = "" Then
                        MyTracing("nothing") 'nothing left to test
                    Else
                        If Language_Grammer(I) = MyConstantIgnoreFunctionOperatorkeyWord & "grammer" Then
                        Else
                            MyTrace(2357, "not a valid grammer, missing ::= " & vbCrLf & Language_Grammer(I))
                        End If
                    End If
                End If
            Next
        End Sub


        'Routine This reads in an file with all of the information for a FlowChart & symbol.
        Public Shared Sub Import(InputFileName As String) ' Yes I know its spelled Import, but I did not want to confuse it between the two
            Dim MyCompleteFileText As String
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 58 Then
                MSG_MyTrace1(1386, "Shared:  Import()")
            End If
            MyUniverse.SysGen.DontAskToAdd = True
            ' Start off with junk
            MyUniverse.MySS = FillImportLine()

            ShowAllForms(ShowWindow1, HideWindow1, HideWindow1, ShowWindow1)
            If Dir(InputFileName) = "" Then
                MSG_Abug(1387, "The file name ", InputFileName, "Could not be found")
            End If
            Using reader As System.IO.TextReader = System.IO.File.OpenText(InputFileName)

                MyCompleteFileText = "" ' Used to get through the first time only
                Do
                    If Len(MyCompleteFileText) < 100 Then
                        MyCompleteFileText &= reader.ReadToEnd() ' add to this text if less than 100 characters long
                    End If
                    MyMakeArraySizesBigger()
                    DisplayMyStatus("Importing:line # " & MyUniverse.MySS.Inputs.LineNumberIn & " : " & MyUniverse.MySS.Inputs.KeyParameters)
                    MyCompleteFileText = ImportLineOrFile(MyCompleteFileText)
                Loop While MyCompleteFileText <> Nothing
            End Using
            SortALLiSAM()
            ShowAllForms(ShowWindow1, HideWindow1, HideWindow1, ShowWindow1)
            MyUniverse.SysGen.DontAskToAdd = False
            'FindingMyBugs(10) 'hack Least amount of checking here
            PaintAll(FlowChartWindow.PictureBox1, 1, TopOfFile("FlowChart")) '20200709
            DisplayMyStatus("Finished Importing.Number of lines =" & MyUniverse.MySS.Inputs.LineNumberIn)
            'FindingMyBugs(10) 'hack Least amount of checking here 'hack
            MyFlowChartNameSpace.F_C.MyUniverse.Grammer = False
            SortAllList()
            AddGrammer()
            SortAllList()
        End Sub

        Public Shared Function PopGrammer(ByRef CodeLine As String) As String
            Dim I As Int32
            CodeLine = MyTrim(CodeLine)
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 61 Then MyTracing(CodeLine)
            ' get the first one.
            'if this is a '?' literial then return it
            If Left(CodeLine, 1) = "'" Then 'return this string 
                I = 2
                While I < Len(CodeLine) And Mid(CodeLine, I, 1) <> "'"
                    I += 1
                End While
                PopGrammer = Mid(CodeLine, 1, I)
                CodeLine = Mid(CodeLine, I + 1, Len(CodeLine))
                Return PopGrammer
            End If
            CodeLine = MyTrim(CodeLine)
            'if this is a grammer name then return it.
            CodeLine = MyTrim(CodeLine)
            PopGrammer = Pop(CodeLine, MyUniverse.SysGen.Constants.ConstantDelimeters)
            If Len(PopGrammer) = 1 Then Return PopGrammer
            Select Case Right(PopGrammer, 1)
                Case "-", "+", "?", "*"
                    'need to find grammer???
                    Return Left(PopGrammer, Len(PopGrammer) - 1)
                Case Else
                    If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 91 Then MyTracing(CodeLine)
            End Select
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 61 Then MyTracing(CodeLine)
            Return PopGrammer
        End Function

