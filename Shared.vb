        Public Shared Function FindingMyBugsSorted() As Int32
            Dim I As Int32
            DisplayMyStatus("Sorting . . . ")
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MSG_MyTrace1(1942, "Shared:  Finding My Bugs: Sorted()")
            End If
            FindingMyBugsSorted = 0
            If MyOptionTest(31) = False Then Exit Function
            DisplayMyStatus("Sorting . . . flowcharts ")
            For I = 1 To TopOfFile("FlowChart") '  UBound(FlowChart_FileCoded)
                FindingMyBugsSorted += FindingMySortedBugNumber(FlowChart_FileX1, FlowChart_iSAM_X1, I)
                FindingMyBugsSorted += FindingMySortedBugNumber(FlowChart_FileY1, FlowChart_iSAM_Y1, I)
                FindingMyBugsSorted += FindingMySortedBugNumber(FlowChart_File_X2_Rotation, FlowChart_iSAM_X2, I)
                FindingMyBugsSorted += FindingMySortedBugNumber(FlowChart_File_Y2_Option, FlowChart_iSAM_Y2, I)
                FindingMyBugsSorted += FindingMySortedBugString(FlowChart_FilePathSymbolName, FlowChart_iSAM_Name, I)
            Next


            DisplayMyStatus("Sorting . . . syntaxs")
            For I = 1 To TopOfFile("named") 'UBound(Named_FileSintax_Isam)
                FindingMyBugsSorted += FindingMySortedBugString(Named_FileSymbolName, Named_FileSymbolName_iSAM, I)
                FindingMyBugsSorted += FindingMySortedBugString(Named_FileSintax, Named_FileSintax_Isam, I)
            Next



            DisplayMyStatus("Sorting . . . datatypes")
            For I = 1 To TopOfFile("Datatype") 'UBound(DataType_FileName)
                FindingMyBugsSorted += FindingMySortedBugString(DataType_FileName, DataType_Name_iSAM, I)
            Next



            DisplayMyStatus("Sorting . . . colors")
            For I = 1 To TopOfFile("Color") 'UBound(Color_FileName)
                FindingMyBugsSorted += FindingMySortedBugString(Color_FileName, Color_Name_iSAM, I)
            Next

            DisplayMyStatus("Sorting . . . keywords")
            For I = 1 + 1 To TopOfFile("keyword") - 1 'UBound(Language_KeyWord) - 1
                If Language_KeyWord(I - 1) >= Language_KeyWord(I) Then FindingMyBugsSorted += 1
                If Language_KeyWord(I) >= Language_KeyWord(I + 1) Then FindingMyBugsSorted += 1
            Next

            DisplayMyStatus("Sorting . . . operators")
            For I = 1 + 1 To TopOfFile("operator") - 1 'UBound(Language_Operator) - 1
                If Language_Operator(I - 1) >= Language_Operator(I) Then FindingMyBugsSorted += 1
                If Language_Operator(I) >= Language_Operator(I + 1) Then FindingMyBugsSorted += 1
            Next

            DisplayMyStatus("Sorting . . . functions")
            For I = 1 + 1 To TopOfFile("function") - 2 'UBound(Language_Function) - 1
                If Language_Function(I - 1) >= Language_Function(I) Then FindingMyBugsSorted += 1
                If Language_Function(I) >= Language_Function(I + 1) Then FindingMyBugsSorted += 1
            Next

            DisplayMyStatus("Sorting . . . grammer ")
            For I = 1 + 1 To TopOfFile("grammer") - 1 'UBound(Language_Grammer) - 1
                If Language_Grammer(I - 1) >= Language_Grammer(I) Then FindingMyBugsSorted += 1
                If Language_Grammer(I) >= Language_Grammer(I + 1) Then FindingMyBugsSorted += 1
            Next
            If FindingMyBugsSorted > 2 Then MyTracing("error ? " & FindingMyBugsSorted.ToString)
        End Function

