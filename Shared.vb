                FileNameOnly = Mid(FileNameOnly, InStr(FileNameOnly, "\") + 1, Len(FileNameOnly))
            End While
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 53 Then MyTracing("-FileNameOnly")
        End Function



