        Public Shared Sub MyTrace(MyBugNumber As Int32, Routine As String)
            If Right(Routine, Len(vbCrLf)) = vbCrLf Then
                While InStr(Routine, vbCrLf & vbCrLf) <> 0
                    Routine = Routine.Replace(vbCrLf & vbCrLf, vbCrLf)
                End While
                System.Diagnostics.Debug.Write("*** " & MyBugNumber.ToString & vbTab & Routine)
            Else
                System.Diagnostics.Debug.Write("*** " & MyBugNumber.ToString & vbTab & Routine & vbCrLf)
            End If
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 10 Then
                Return
            End If
        End Sub ' mytrace

