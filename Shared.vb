        'If the passed index is zero, then those function can not/are not checked
        Public Shared Sub FindingMyBugsCheckForErrors(IndexFlowChart As Int32, IndexNamed As Int32, IndexSymbol As Int32) ' Checks for common errors
            Dim BitFlagX, Cdex As Int32
            Dim I, J, K As Int32
            Dim DataType, PointName, SinTax, Temp As String
            Dim MyXY As MyPointStructure = Nothing
            Dim MyArray(256) As String

            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 87 Then
                If IndexFlowChart <> 0 Then MSG_MyTrace3(1851, MyShowFlowChartRecord(False, IndexFlowChart))
                If IndexNamed <> 0 Then MSG_MyTrace3(1852, MyShowNamedRecords(False, IndexNamed))
                If IndexSymbol <> 0 Then MSG_MyTrace3(1853, MyShowSymbolRecords(False, IndexSymbol))
            End If

            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 99 Then
                'MSG_MyTrace1(1854, "Shared: Finding My Bugs: Check For Errors")
                If Named_TableSymbolName(IndexNamed) <> Symbol_TableSymbolName(IndexSymbol) Then
                    Dump4()
                    MSG_Abug(1855, "FindingMyBugsCheckForErrors() : index to name of symbol does not match the sybol name of the index to the graphics " & vbCrLf & Named_TableSymbolName(IndexNamed) & "<------>" & Symbol_TableSymbolName(IndexSymbol), vbCrLf & IndexNamed.ToString & vbTab & MyShowNamedRecords(False, IndexNamed), vbCrLf & IndexSymbol.ToString & vbTab & MyShowSymbolRecords(False, IndexSymbol))
                End If
            End If
            If MyOptionTest(31) = False Then Exit Sub

            BitFlagX = 0
            'Cdex = 0
            If InStr(MyUniverse.SysGen.Constants.SyntaxCameFromLine, Named_TableSintax(IndexNamed)) = 0 Then
                BitFlagX = BitFlagX Or 2 ' has a come from in the syntax (even if remarked out
            End If
            If InStr(MyUniverse.SysGen.Constants.SyntaxGotoNextLine, Named_TableSintax(IndexNamed)) = 0 Then
                BitFlagX = BitFlagX Or 4 ' has a come from in the syntax ...
            End If
            Select Case BitFlagX
                Case 2
                    '1002 Symbol does not have a /point GotoNextLine
                    MyFlowChartErrors(1002, 0, 0, Cdex, " Symbol " & FindSymbolName(IndexSymbol))
                Case 4
                    '1001 Symbol does not have a /point CameFromLine
                    MyFlowChartErrors(1001, 0, 0, Cdex, " Symbol " & FindSymbolName(IndexSymbol))
                Case 6 ' It has them both so this error is ok
                Case Else
                    MSG_Ainfo(1856, "Error bits ", BitFlagX.ToString, "")
            End Select 'checking if the symbol has CameFromLine and a GotoNextLine

            If IndexSymbol > 1 Then
                Cdex = IndexSymbol
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 87 Then
                    If IndexFlowChart <> 0 Then MSG_MyTrace3(1857, ", Flow Chart Record =" & vbCrLf & MyShowFlowChartRecord(False, IndexFlowChart))
                    If IndexNamed <> 0 Then MSG_MyTrace3(1858, ", Named record =" & MyShowNamedRecords(False, IndexNamed))
                    If IndexSymbol <> 0 Then MSG_MyTrace3(1859, ", symbol=" & vbCrLf & MyShowSymbolGraphic(False, IndexSymbol))
                End If
                While Cdex > 0 And Symbol_TableCoded_String(Cdex) <> "/name"
                    If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
