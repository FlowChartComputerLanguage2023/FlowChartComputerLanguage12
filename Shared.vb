        Public Shared Function MyShowDataTable(expanded As Boolean, Idex As Int32) As String
            If Len(DataType_FileName(Idex)) < 1 Then
                MyShowDataTable = "/ignore \Index=" & Idex & " Invalid Data Type Name at " & Idex.ToString
                Exit Function
            End If

            If DataType_TableColorIndex(Idex) < 1 Then
                MyShowDataTable = vbCrLf & "/ignore \Index=" & Idex & " Invalid color for Data Type Name " & DataType_TableName(Idex)
                Exit Function
            End If

            '"/datatype=datatypename" & FD & " Number Of Bytes" & FD & " Color Name" & FD & " Color Width" & FD & " Describtion"
            If expanded = True Then
                MyShowDataTable = "/DataType=" & DataType_FileName(Idex)
                MyShowDataTable &= FD & DataType_FileNumberOfBytes(Idex)
                MyShowDataTable &= FD & FindColorFromDataType(DataType_FileName(Idex))
                MyShowDataTable &= FD & DataType_FileWidth(Idex)
                MyShowDataTable &= FD & DataType_FileDescribtion(Idex)
                MyShowDataTable &= FD & "\Index=" & Idex
                MyShowDataTable &= FD & DataType_TableColorIndex(FindDataTypeIndex(DataType_FileName(Idex)))
                MyShowDataTable &= FD & MyShowColor(DataType_TableColorIndex(FindDataTypeIndex(DataType_FileName(Idex))))
            Else
                MyShowDataTable = "/DataType=" & DataType_FileName(Idex)
                MyShowDataTable &= FD & DataType_FileNumberOfBytes(Idex) & " # bytes"
                MyShowDataTable &= FD & FindColorFromDataType(DataType_FileName(Idex)) & " color"
                MyShowDataTable &= FD & DataType_FileWidth(Idex) & " size"
                MyShowDataTable &= FD & DataType_FileDescribtion(Idex) & FD & " \Describtion"
                MyShowDataTable &= FD & Idex.ToString & " \index"
            End If
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MSG_MyTrace1(1642, "Shared:MyShow Data Table()" & vbCrLf & MyShowDataTable)
            End If
        End Function


