                End If
            ElseIf IndexColor > 0 Then
                'TempSize = 2
                ColorNameIs = MyColorName
                '?IndexDataType = constantMyErrorCode
            End If

            TempSize = FindMySize(DataTypeName, ColorNameIs) 'size is only valid for datatypes
