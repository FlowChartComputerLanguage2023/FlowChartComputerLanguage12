            If Left(CodeLine, Len(MyUniverse.SysGen.Constants.SyntaxAlphas)) = MyUniverse.SysGen.Constants.SyntaxAlphas Then Return MyUniverse.SysGen.Constants.SyntaxAlphas
            If Left(CodeLine, Len(MyUniverse.SysGen.Constants.SyntaxGotoNextLine)) = MyUniverse.SysGen.Constants.SyntaxGotoNextLine Then Return MyUniverse.SysGen.Constants.SyntaxGotoNextLine
            If Left(CodeLine, Len(MyUniverse.SysGen.Constants.SyntaxCameFromLine)) = MyUniverse.SysGen.Constants.SyntaxCameFromLine Then Return MyUniverse.SysGen.Constants.SyntaxCameFromLine
            If Left(CodeLine, Len(MyUniverse.SysGen.Constants.SyntaxComments)) = MyUniverse.SysGen.Constants.SyntaxComments Then Return MyUniverse.SysGen.Constants.SyntaxComments
            X = MyUniverse.ProgramOptions.C_L_MultiLine : If Left(CodeLine, Len(X)) = X Then Return X
            X = MyUniverse.ProgramOptions.C_L_Comment : If Left(CodeLine, Len(X)) = X Then Return X
            X = MyUniverse.ProgramOptions.C_L_Extensions : If Left(CodeLine, Len(X)) = X Then Return X
            '''''''''''    If Left(CodeLine, Len(MyUniverse.SysGen.Constants.SyntaxVariables)) = MyUniverse.SysGen.Constants.SyntaxVariables Then Return MyUniverse.SysGen.Constants.SyntaxVariables
            If Left(CodeLine, Len(MyUniverse.SysGen.Constants.RMStart)) = MyUniverse.SysGen.Constants.RMStart Then
                If InStr(CodeLine, MyUniverse.SysGen.Constants.RMEnd) <> 0 Then
                    If InStr(CodeLine, ".") < InStr(CodeLine, MyUniverse.SysGen.Constants.RMEnd) Then
                        Return Left(CodeLine, InStr(CodeLine, MyUniverse.SysGen.Constants.RMEnd) + 2)
                    End If
                End If
            End If
            If IsThisAMarker2(CodeLine) <> "" Then Return IsThisAMarker2(CodeLine) ' Returns the length of this marker format Myfd(point.name )
            ' All special characters are after language special characters
            If Left(CodeLine, Len(MyUniverse.SysGen.Constants.SyntaxSpecialCharacters)) = MyUniverse.SysGen.Constants.SyntaxSpecialCharacters Then Return MyUniverse.SysGen.Constants.ConstantSpecialCharacters

            Return IsThisAMarker2(CodeLine) ' Not a marker but might still be one of this kind
