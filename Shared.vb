            If Right(t, 1) = "+" Then t = Mid(t, 1, Len(t) - 1)
            If Right(t, 1) = "-" Then t = Mid(t, 1, Len(t) - 1)

            For IndexGrammer = LBound(Language_Grammer) To UBound(Language_Grammer)
                s = Language_Grammer(IndexGrammer)
                s = Pop(s, MyUniverse.SysGen.Constants.ConstantDelimeters)
                If s = t Then
