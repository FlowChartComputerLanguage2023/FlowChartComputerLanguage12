        Public Shared Sub Pan2(x As Int32, y As Int32)
            Dim A, B, C, D, E, F, OLD As MyPointStructure
            Dim S As String
            OLD.Xx = MyUniverse.MyMouseAndDrawing.MyScrenZeroZero.Xx
            OLD.Yy = MyUniverse.MyMouseAndDrawing.MyScrenZeroZero.Yy
            MyUniverse.MyMouseAndDrawing.MyScrenZeroZero.Xx = 0
            MyUniverse.MyMouseAndDrawing.MyScrenZeroZero.Yy = 0
            A = Nothing
            'removed not used ?  B = Nothing
            'removed not used ?  C = Nothing
            D = Nothing
            'removed not used ?  E = Nothing
            F = Nothing

            S = "Move center to " & MyShowXY(x, y)
            A.Xx = FlowChartWindow.Width
            A.Yy = FlowChartWindow.Height
            S &= "window size " & MyShowPoint(A)
            B = Copy2WorldXY("", FlowChartWindow.PictureBox1, A)
