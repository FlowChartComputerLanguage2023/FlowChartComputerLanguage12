        Public Shared Sub LimitScale()
            If MyUniverse.SysGen.MyFlowChartScale < ConstantMinFLowChartScale Then
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 29 Then
                    MSG_MyTrace1(1082, "Shared:  Limit Scale()" & ConstantMinFLowChartScale.ToString & ", " & MyUniverse.SysGen.MyFlowChartScale.ToString & ", " & ConstantMaxFlowChartScale.ToString & "  <--->  " & 1.ToString & ", " & MyUniverse.SysGen.MySymbolScale.ToString & ", " & 8.ToString & vbCrLf)
                End If
                MyUniverse.SysGen.MyFlowChartScale = ConstantMinFLowChartScale '1/16
                MSG_MyTrace1(1083, "Shared:  Scaled Size changed ()" & MyShowScale() & vbCrLf)
                'todo   ''''' DisplayStatus(FlowChartWindow.LabelProgramStatus.Text, "FlowCHart Scale limit is " & CStr(MyUniverse.SysGen.MyFlowChartScale))
            End If
            If MyUniverse.SysGen.MyFlowChartScale > ConstantMaxFlowChartScale Then
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 29 Then
                    MSG_MyTrace1(1084, "Shared:  Limit Scale()" & ConstantMinFLowChartScale.ToString & ", " & MyUniverse.SysGen.MyFlowChartScale.ToString & ", " & ConstantMaxFlowChartScale.ToString & "  <--->  " & 1.ToString & ", " & MyUniverse.SysGen.MySymbolScale.ToString & ", " & 8.ToString & vbCrLf)
                End If
                MyUniverse.SysGen.MyFlowChartScale = ConstantMaxFlowChartScale
                MSG_MyTrace1(1085, "Shared:  Scaled Size changed ()" & MyShowScale() & vbCrLf)
                DisplayStatus(FlowChartWindow.LabelProgramStatus.Text, "FlowChart Scale limit is " & CStr(MyUniverse.SysGen.MyFlowChartScale))
            End If
            'todo need to also limit the symbol scale from 1 to 4?
            If MyUniverse.SysGen.MySymbolScale > 8 Then
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 29 Then
                    MSG_MyTrace1(1086, "Shared:  Limit Scale()" & ConstantMinFLowChartScale.ToString & ", " & MyUniverse.SysGen.MyFlowChartScale.ToString & ", " & ConstantMaxFlowChartScale.ToString & ",   <--->  " & 1.ToString & ", " & MyUniverse.SysGen.MySymbolScale.ToString & ", " & 8.ToString & vbCrLf)
                End If
                MyUniverse.SysGen.MySymbolScale = 2
                MSG_MyTrace1(1087, "Shared:  Scaled Size changed ()" & MyShowScale() & vbCrLf)
                DisplayStatus(MyUniverse.SysGen.MySymbolScale.ToString, "Symbol Scale limit is " & MyUniverse.SysGen.MySymbolScale.ToString)
            End If
            If MyUniverse.SysGen.MySymbolScale < 1 Then
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 29 Then
                    MSG_MyTrace1(1088, "Shared:  Limit Scale()" & ConstantMinFLowChartScale.ToString & ", " & MyUniverse.SysGen.MyFlowChartScale.ToString & ", " & ConstantMaxFlowChartScale.ToString & "  <--->  " & 1.ToString & ", " & MyUniverse.SysGen.MySymbolScale.ToString & ", " & 8.ToString & vbCrLf)
                End If
                MyUniverse.SysGen.MySymbolScale = 1
                MSG_MyTrace1(1089, "Shared:  Scaled Size changed ()" & MyShowScale())
                DisplayStatus(MyUniverse.SysGen.MySymbolScale.ToString, "Symbol Scale limit is " & MyUniverse.SysGen.MySymbolScale.ToString)
            End If
        End Sub


