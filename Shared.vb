            End If

            MSG_Ainfo1(1402, "Moving symbol " & Index, MyShowFlowChartRecord(False, Index), "To " & MyPoint1XY(FlowChart_TableX1(Index) + XDist, FlowChart_TableY1(Index) + YDist).ToString)
