                AddFlowChartRecordPath(PathName, "/path", P_A.Xx, P_A.Yy, NextPointXY.Xx, NextPointXY.Yy, "DataType", "")
                Return True
            End If

            'todo in the future
            '2.0 try stepping across the shortest path looking for one that goes the the X or Y of B
            MSG_Abug(1776, "Attempted path not clear", "", "")
            Return False
        End Function

