        Public Shared Function ImportLineOrFile(InputLineOrFile As String) As String
            Dim I As Int32
            Dim Temp, Temp1, Temp2, Temp3 As String
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 51 Then
                MyTracing("@ImportLineOrFile ")
                MSG_MyTrace1(1338, "Shared:  Import Line Or File()   " & Len(InputLineOrFile).ToString)
            End If
            MyMakeArraySizesBigger()
            'process all 
            'make sure that i include anything left over not done.
            MyUniverse.MySS.Inputs.KeyLineOrFile = InputLineOrFile

            While Len(MyUniverse.MySS.Inputs.KeyLineOrFile) > 0
                MyDoEvents()
                MyUniverse.MySS.Inputs.KeyLineOrFile = MyTrim(MyUniverse.MySS.Inputs.KeyLineOrFile)
                MyUniverse.MySS.Inputs.KeyParameters = PopLine(MyTrim(MyUniverse.MySS.Inputs.KeyLineOrFile))
                MyUniverse.MySS.Inputs.KeyLineOrFile = Mid(MyUniverse.MySS.Inputs.KeyLineOrFile, 1 + Len(MyUniverse.MySS.Inputs.KeyParameters), Len(MyUniverse.MySS.Inputs.KeyLineOrFile))
                Temp = MyUniverse.MySS.Inputs.KeyLineOrFile
                'Temp1 = MyUniverse.MySS.Inputs.KeyParameters
                MyUniverse.MySS.Inputs.KeyWord = MyReplace(Pop(MyUniverse.MySS.Inputs.KeyParameters, "="), "=", "")
                If MyUniverse.MySS.Inputs.LineNumberIn <> 0 And Int(MyUniverse.MySS.Inputs.LineNumberIn / 100) * 100 = MyUniverse.MySS.Inputs.LineNumberIn Then
                    DisplayMyStatus("At Line " & MyUniverse.MySS.Inputs.LineNumberIn & MyUniverse.ProgramOptions.C_L_WhiteSpace & MyUniverse.MySS.Inputs.KeyWord & "=" & MyUniverse.MySS.Inputs.KeyParameters)
                End If
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 43 Then
                    MSG_MyTrace1(1339, MyUniverse.MySS.Inputs.KeyWord & "=" & MyUniverse.MySS.Inputs.KeyParameters)
                End If
                Temp2 = MyTrim(MyUniverse.MySS.Inputs.KeyWord)
                Select Case LCase(MyTrim(MyUniverse.MySS.Inputs.KeyWord))
                    Case "/grammer"
                        MyUniverse.Grammer = True 'This is to tell that a grammer has been entered.
                        MyUniverse.MySS.Inputs.KeyParameters = FixGrammer(MyUniverse.MySS.Inputs.KeyParameters)
                        Temp3 = MyTrim(MyUniverse.MySS.Inputs.KeyParameters)
                        If InStr(MyUniverse.MySS.Inputs.KeyParameters, "::=") <> 0 Then
