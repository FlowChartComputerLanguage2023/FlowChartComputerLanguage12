        Public Shared Function GetHex(A As String) As String
            Dim I As Int32
            GetHex = ""
            I = InStr(A, "0x") + 2
            While IsThisAHexDigit(Mid(A, I, 1)) = True
                GetHex &= Mid(A, I, 1)
                I += 1
            End While
            Return "0x" & GetHex
        End Function


        Public Shared Function FixGrammer(Code_line As String) As String ' This will change '<0x????>' and '<0x????-0x????>' to characters
            Dim I As Int32
            Dim Temp As String
            FixGrammer = MyTrim(Code_line)
            FixGrammer = FixGrammer.Replace("&h", "0x") 'change from basic default to other HEX
            While InStr("0x", FixGrammer) > 0
                Temp = GetHex(FixGrammer)
                'Temp = Mid(FixGrammer, InStr(FixGrammer, "0x"), 6) 'todo sometimes it is a bigger than 6 characters (16 bits)
                I = Hex2Bin(Temp)
                If I >= -32768 And I <= 65535 Then
                    FixGrammer = FixGrammer.Replace(Temp, ChrW(I))
                Else
                    'MSG_Ainfo1(2345, "Hex is outside of character range -32768 65535 " & Temp & HighLight(Char.ConvertFromUtf32(I)), I.ToString, "")
                    FixGrammer = FixGrammer.Replace(Temp, Char.ConvertFromUtf32(I))
                End If
            End While
            Return FixGrammer
        End Function


