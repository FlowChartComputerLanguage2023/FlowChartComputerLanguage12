        Public Shared Function Pop(ByRef A As String, Delimiters As String) As String ' Returns the first "word" and never the seperating character(s)
            Dim X As String
            Dim Z As Int32
            'MyTracing("@Pop")
            Pop = A
            Z = MyFirstOne(Pop, Delimiters)
            If Z > 0 Then
                Pop = Mid(A, 1, Z)
                A = Mid(A, Z + 1, Len(A))
            Else
                Pop = A
                A = ""
            End If
            If Len(Pop) > 1 Then
                Pop = MyTrim(Pop)
            End If
            Return Pop





            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 99 Then
                MSG_MyTrace1(1252, "Shared:  Pop()-->" & A & "<--" & Mid(A, 1, Z))
            End If
            A = Trim(A)
            X = Left(A, 1)
            A = Mid(A, 2, Len(A))
            ' This needs to be changed so that we find where the trim is, and then move the string only once, becuse this way is VERY slow.
            While InStr(Delimiters, Left(A, 1)) = 0
                X += Left(A, 1)
                A = Mid(A, 2, Len(A))
            End While
            Pop = X
            A = Trim(Mid(A, 2, Len(A))) ' to get rid of the character that stopped the pop
            'MyTracing("-Pop")
