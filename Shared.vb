        Public Shared Function PopQuote(ByRef A As String) As String
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MSG_MyTrace1(1253, "Shared:  Pop Quote()")
            End If
            If Left(A, Len(MyUniverse.SysGen.Constants.SyntaxQuotes)) = MyUniverse.SysGen.Constants.SyntaxQuotes Then
                A = Mid(A, Len(MyUniverse.SysGen.Constants.SyntaxQuotes) + 1, Len(A))
                Return MyUniverse.SysGen.Constants.ConstantQuote
            End If
            PopQuote = Left(A, 1) 'First quote
            A = Mid(A, 2, Len(A))
            While Left(A, 1) <> Chr(34) And Len(A) > 0
                PopQuote &= Left(A, 1)
                A = Mid(A, 2, Len(A))
                If InStr(A, Chr(34)) <> 0 Then
                    Return MyUniverse.SysGen.Constants.ConstantQuote
                End If
            End While
            ' Save The Last Quote also
            'todo is this nessary '''PopQuote &= Left(A, 1)
            A = Mid(A, 2, Len(A))
            Return MyUniverse.SysGen.Constants.ConstantQuote
        End Function


