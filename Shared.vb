        Public Shared Function FindWidthFromDataType(DataTypeName As String) As Int32
            Dim I2 As Int32
            Dim Idex, Kdex, Jdex As Int32
            Dim ErrorCount As Int32
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 55 Then
                MSG_MyTrace1(1502, "Shared:  Find Width From Data Type = " + DataTypeName)
            End If
            Kdex = TopOfFile("DataType")
            Idex = CInt(Kdex / 2)
            Jdex = CInt((Idex - 4) / 2)
            ErrorCount = 2048
            FindWidthFromDataType = 0
            While ErrorCount > 0
                ErrorCount -= 1
                Select Case MyCompared3(DataType_TableName(DataType_Name_iSAM(Idex)), DataTypeName, DataType_TableName(DataType_Name_iSAM(Idex + 1)))
                    Case -5 ' Test 9 A > C unsorted list error
                        MSG_Abug(1503, DataType_TableName(DataType_Name_iSAM(Idex)), DataTypeName, DataType_TableName(DataType_Name_iSAM(Idex + 1)))
                        FindWidthFromDataType = 1 'default min datatype/line width
                        Idex -= Jdex
                    Case -4 'Test 5 & 7  both A=nothing and b = c then only A=nothing
                        FindWidthFromDataType = 0
                        'todo should this one be + or -?
                        Idex += 1
                    Case -3 'test 11 A is higher than B  and C = nothing
                        FindWidthFromDataType = 1 'default min datatype/line width
                        Idex -= Jdex
                    Case -2 'test 12  b>C
                        FindWidthFromDataType = 1 'default min datatype/line width
                        Idex += Jdex
                    Case -1 'Test 3 -> A = b
                        Return DataType_TableWidth(Idex)
                    Case 0 'Test 2 & 10 A and C are both null or nothing then A<b<C not in list
                        FindWidthFromDataType = 0
                        Exit While
                    Case 1 'Test 4 -> b=C so move forward just one.
                        FindWidthFromDataType = DataType_TableWidth(Idex)
                        Idex += 1
                    Case 2 'test 14 A<b
                        FindWidthFromDataType = DataType_TableWidth(Idex)
                        Idex += Jdex
                    Case 3 'test 13 b < C
                        FindWidthFromDataType = DataType_TableWidth(Idex)
                        Idex += Jdex
                    Case 4 'test 6 & 8 ->>> C is nothing and b > A then C=nothing
                        FindWidthFromDataType = 0
                        Exit While
                    Case 5 'Test 1 & 15 -> b=nothing then no other test works (Error)
                        MSG_Abug(1504, DataType_TableName(DataType_Name_iSAM(Idex)), "searching for" & HighLight(DataTypeName), DataType_TableName(DataType_Name_iSAM(Idex + 1)))
                        Return 1
                End Select
                If Idex = 0 And Jdex = 1 Then Exit While
                Idex = MyMinMax(Idex, 1, Kdex)
                Jdex = MyMinMax(CInt(Jdex / 2), 1, Kdex)
            End While
            For I2 = 1 To TopOfFile("DataType")
                If LCase(Trim(DataTypeName)) = LCase(Trim(DataType_TableName(I2))) Then
                    MSG_Abug(1505, "Failed binary search for width", DataTypeName, "")
                    Return DataType_TableWidth(I2)
                End If
            Next
        End Function


        'returns if the colors have changed.
        Public Shared Function TestIfColorStandardMicroSoftColorName(IndexColor As Int32) As Int32
            Dim I As Int32
            Dim R1, R2, G1, G2, B1, B2, A1, A2, LastIndex As Int32
            Dim LastDistance, Distance As Int64
            Dim ThisPen, ThatPen As Pen
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 51 Then
                MSG_MyTrace3(1506, "Shared:  Test If Color Standard Micro Soft Color Name() " & vbCrLf & MyShowColor(IndexColor))
            End If
            If Len(Color_TableName(IndexColor)) = 0 Then Return constantMyErrorCode
            '?LastIndex = LBound(Color_FileName)
            ThisPen = MyGetPen_Static(Color_TableName(IndexColor), "")

            If ThisPen.Color.Name = Color_TableName(IndexColor) Then Return IndexColor
            ThatPen = MyGetPen_Static(Color_TableName(IndexColor), "")
            'todo if this color matches then we do not need to find the closest
            MSG_Abug(1507, "Color Name Error: " & HighLight(ThisPen.Color.Name) & "<>" & HighLight(ThatPen.Color.Name) & " Index= " & IndexColor, Color_TableName(IndexColor), ThisPen.Color.Name.ToString)

            R1 = ThisPen.Color.R
            G1 = ThisPen.Color.G
            B1 = ThisPen.Color.B
            A1 = ThisPen.Color.A
            ' go through all of the pens to see if this one is there.

            LastDistance = 32000
            LastIndex = 1

            For I = 1 To UBound(Color_FileName)
                If Len(Color_FileName(I)) <> 0 Then
                    ThatPen = MyGetPen_Static(Color_TableName(I), "")
                    'Th color is unknown so find the closest
                    ' go through all of the pens to see if this one is there.
                    R2 = ThatPen.Color.R
                    G2 = ThatPen.Color.G
                    B2 = ThatPen.Color.B
                    A2 = ThatPen.Color.A
                    Distance = MyABS64(R1 - R2)
                    Distance += MyABS64(G1 - G2)
                    Distance += MyABS64(B1 - B2)
                    Distance += MyABS64(A1 - A2)
                    If Distance < LastDistance Then
                        LastIndex = I
                        LastDistance = Distance
                    End If
                    If ThatPen.Color.Name = ThisPen.Color.Name Then
                        Return I ' names match
                    End If
                End If
            Next
            'This is the closest they come
            Return LastIndex
        End Function




