        Public Shared Function My_VariableName(CodeLine As String, NumberOfTheVariable As Int32) As String 'This should return the name of the variabl
            'ERROR this is not getting the correct variable name
            Dim MyArray(1) As String
            Dim Idex As Int32
            Dim PointName1, PointName2 As String
            Dim Temp As String
            Temp = PopLine(MyNoCR(CodeLine))
            MyUniverse.MyStaticData.Path_Names = MyParse(MyArray, CodeLine)
            MyUniverse.MyStaticData.Path_NamesCounted = CountPathNames(MyUniverse.MyStaticData.Path_Names)

            PointName1 = MyUniverse.MyStaticData.Path_Names
            FMB(MyUniverse.MyStaticData.Path_Names)
            For Idex = 1 To NumberOfTheVariable
                PointName2 = Pop(PointName1, FD)
                FMB(PointName1)
                FMB(PointName2)
                If Idex = NumberOfTheVariable Then
                    'PointName1 = MyUniverse.MyStaticData.Path_Names  'hack debug only 
                    PointName2 = NoFD(PointName2)
                    FMB(PointName1)
                    FMB(PointName2)
                    If Len(PointName2) < 1 Then
                        FMB(MyUniverse.MyStaticData.Path_Names)
                        MSG_Abug(1221, "Check the variable numbered   <--->  " & HighLight(PointName1) & vbCrLf & HighLight(PointName2), NumberOfTheVariable.ToString & vbCrLf & MyUniverse.MyStaticData.Path_Names & vbCrLf & " variable is not in the code line" & "  <--->  " & CodeLine, "")
                        My_VariableName = MakePathName("VariableName_" & Right(MyRnd(), 6))
                        MyUniverse.MyStaticData.Path_Names &= My_VariableName & FD
                        MyUniverse.MyStaticData.Path_NamesCounted = CountPathNames(MyUniverse.MyStaticData.Path_Names)
                        'todo make up a name, because the list of names is shorter than the one requested
                        FMB(MyUniverse.MyStaticData.Path_Names)
                        MyUniverse.MyStaticData.Path_Names &= FD & My_VariableName & FD
                        Return NameOfPointOnly(My_VariableName)
                    End If
                    Return NameOfPointOnly(PointName2)
                End If
            Next


            Idex = UBound(MyArray)
            If NumberOfTheVariable > Idex Then
                MSG_Ainfo1(1222, "The number of points is greater then the number of items parsed", Idex, NumberOfTheVariable)
                Return NoWhiteSpaceS(CodeLine) & "___" & NumberOfTheVariable & "___" & Idex
            End If
            'My_VariableName = "_" & NoWhiteSpaceS(MyArray(NumberOfTheVariable)) & "_" & NumberOfTheVariable & "_" & MyUniverse.MySS.Inputs.LineNumberIn ' This Should Never Be used
            My_VariableName = "V_2_"
            For Idex = 1 To UBound(MyArray)
                Select Case LCase(IsThisAWhat(MyArray(Idex)))
                    Case MyUniverse.ProgramOptions.C_L_MultiLine
                        Exit For
                    Case MyUniverse.SysGen.Constants.SyntaxGotoNextLine ' These requires a point on the symbol 2020 08 26
                        If NumberOfTheVariable = Idex Then
                            Return MyArray(Idex) '+1))
                        End If
                    Case MyUniverse.SysGen.Constants.SyntaxCameFromLine
                        If NumberOfTheVariable = Idex Then
                            Return MyArray(Idex) '+1) )
                        End If
                    Case MyUniverse.ProgramOptions.C_L_Comment
                        MSG_Ainfo(1223, IsThisAWhat(MyArray(Idex)) & " (comment) Is ignored", Idex, "")
                    Case MyUniverse.ProgramOptions.C_L_Extensions
                        MSG_Ainfo(1224, IsThisAWhat(MyArray(Idex)) & " Is ignored", Idex, "")
                    Case MyUniverse.ProgramOptions.C_L_VarChars
                        MSG_Ainfo(1225, IsThisAWhat(MyArray(Idex)) & " Is ignored", Idex, "")
                    Case MyUniverse.SysGen.Constants.SyntaxNumbers 'missing 2020 09 28
                        My_VariableName &= "0"
                        If NumberOfTheVariable = Idex Then
                            Return MyArray(Idex) & "_" & NumberOfTheVariable
                        End If
                    Case MyUniverse.SysGen.Constants.SyntaxAlphas 'missing  2020 09 28
                        My_VariableName &= "A"
                        If NumberOfTheVariable = Idex Then
                            ' A I N F O 2 (134)
                            Return MyArray(Idex)  '& "_" & NumberOfTheVariable ' took back out 2020 08 20
                        End If
                    Case MyUniverse.SysGen.Constants.SyntaxQuotes
                        ' Need to change this to make a Quote a /constant and the name of the point Quote_LineNumber
                        'My_VariableName = MyArray(Idex) '& "_" & NumberOfTheVariable ' took back out 2020 08 20
                        My_VariableName &= "Q"
                        If NumberOfTheVariable = Idex Then
                            Return "Quote_" & MyUniverse.MySS.Inputs.LineNumberIn
                        End If
                    Case MyUniverse.SysGen.Constants.SyntaxKeyWords, MyUniverse.SysGen.Constants.SyntaxOperators, MyUniverse.SysGen.Constants.SyntaxFunctions ' Ignore these keyWord types' Added 2021/01/05        '
                        My_VariableName &= MyArray(Idex)
                    Case Nothing
                        My_VariableName &= ""
        'ignore FD's     Exit Function
                    Case MyUniverse.SysGen.Constants.SyntaxFieldDelimiters
                        My_VariableName &= ""
        'ignore FD's     Exit Function
                    Case MyUniverse.SysGen.Constants.SyntaxSpecialCharacters
                        My_VariableName &= "_"
                    Case Else
                        If IsThisAVariableName(MyArray(Idex)) = True Then
                            If NumberOfTheVariable = Idex Then
                                'return just the name of this
                                Return NameOfPointOnly(MyArray(Idex))
                                'Return MyArray(Idex)
                            End If
                        Else
                            MSG_Abug(1226, "Unknown Syntax key " & HighLight(IsThisAWhat(MyArray(Idex))) & " In My_Variable Name at " & Idex, HighLight(MyArray(Idex)), HighLight(CodeLine))
                        End If
                End Select
            Next Idex
            FMB(MyUniverse.MyStaticData.Path_Names)
            Return MakeNextName(My_VariableName)
        End Function



        Public Shared Function KeyWordForCodeLine(codeline As String) As String
            Dim Temp(1) As String
            Dim I As Int32
            KeyWordForCodeLine = ""
            MyUniverse.MyStaticData.Path_Names = MyParse(Temp, codeline) ' Parse the code line then
            MyUniverse.MyStaticData.Path_NamesCounted = CountPathNames(MyUniverse.MyStaticData.Path_Names)
            For I = 1 To UBound(Temp)
                Select Case IsThisAWhat(Temp(I))
                    Case MyUniverse.SysGen.Constants.SyntaxKeyWords, MyUniverse.SysGen.Constants.SyntaxFunctions
                        KeyWordForCodeLine &= Temp(I)
                    Case MyUniverse.SysGen.Constants.SyntaxCameFromLine ' Ignore for a name
                        KeyWordForCodeLine &= "1"
                    Case MyUniverse.SysGen.Constants.SyntaxGotoNextLine ' Ignore for a name
                        KeyWordForCodeLine &= "2"
                    Case MyUniverse.SysGen.Constants.SyntaxOperators
                        KeyWordForCodeLine &= Temp(I)
                    Case MyUniverse.SysGen.Constants.SyntaxSpecialCharacters
                        KeyWordForCodeLine &= "_"
                    Case MyUniverse.SysGen.Constants.SyntaxQuotes
                        KeyWordForCodeLine &= "Q"
                    Case MyUniverse.ProgramOptions.C_L_MultiLine
                        KeyWordForCodeLine &= ""'Multiline not allowed right now"
                    Case Nothing
                        'MSG_Ainfo1(1227,  "Nothing, comma", IsThisAWhat(Temp(I)), "")
                        KeyWordForCodeLine &= ""
                    Case MyUniverse.SysGen.Constants.SyntaxFieldDelimiters, FD
                        'MSG_Ainfo1(1228,  "Nothing, comma", IsThisAWhat(Temp(I)), "")
                        KeyWordForCodeLine &= "_"
                    Case MyUniverse.ProgramOptions.C_L_Comment
                    Case MyUniverse.ProgramOptions.C_L_Extensions
                    Case MyUniverse.ProgramOptions.C_L_MultiLine
                        KeyWordForCodeLine &= ":"
                    Case MyUniverse.ProgramOptions.C_L_VarChars
                    Case MyUniverse.SysGen.Constants.SyntaxComments
                    Case Else
                        KeyWordForCodeLine &= NameOfPointOnly(Temp(I))
                End Select
            Next I
        End Function

