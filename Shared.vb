        Public Shared Function FindingMyBugsGrammer(What As String, MyArray() As String) As Int32
            Dim IndexGrammer As Int32 'Index, 
            Dim Status, X As String
            Dim Grammer As String
            Dim GrammerKeyWord As String
            Dim Temp1 As Int32
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 45 Then MyTracing("@FMBGrammer")
            If UBound(MyArray) < 4 Then
                Return 0
            End If
            'removed not used ?  FindingMyBugsGrammer = 0
            'todo need to add a section to check for valid hex formats
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 59 Then
                MSG_MyTrace1(2055, "Shared:  Finding My Bugs: Grammer(" & NoRM(What) & ")")
            End If
            FindingMyBugsGrammer = 0
            If MyOptionTest(31) = False Then Exit Function
            For IndexGrammer = LBound(MyArray) + 1 To UBound(MyArray) - 1
                'first make sure that no BNF keyword is a keyword of the language
                Grammer = MyArray(IndexGrammer)
                FMB(Grammer)
                Temp1 = InStr(Grammer, "::=")
                If Temp1 > 0 Then
                    GrammerKeyWord = MyTrim(Left(Grammer, Temp1 - 1))
                Else
                    GrammerKeyWord = Grammer
                End If
                FMB(GrammerKeyWord)
                If IsThisA_KeyWord(GrammerKeyWord) = True Then
                    MSG_Abug(2300, IsThisA_KeyWord(MyArray(IndexGrammer)).ToString, "You can not have your computer language keyword be defined as a grammer keyword.", MyArray(IndexGrammer))
                End If
                If PrintAbleNull(MyArray(IndexGrammer)) <> "_" Then
                    If MyGrammerName(MyArray(IndexGrammer - 1)) = MyGrammerName(MyArray(IndexGrammer)) Then
                        FindingMyBugsGrammer += MSG_Abug(2119, "Finding My Bugs: Program Duplication of Grammer defination is not allowed ", IndexGrammer.ToString, HighLight(MyArray(IndexGrammer - 1)) & HighLight(MyArray(IndexGrammer)))
                    End If
                    If MyArray(IndexGrammer - 1) > MyArray(IndexGrammer) Then
                        FindingMyBugsGrammer += MSG_Abug(2120, "Finding My Bugs: Program out of order of key word", IndexGrammer.ToString, HighLight(MyArray(IndexGrammer - 1)) & HighLight(MyArray(IndexGrammer)))
                    End If
                Else
                    FindingMyBugsGrammer += MSG_Abug(2058, "Finding My Bugs: Void grammer defination " & IndexGrammer.ToString, HighLight(IsThisAWhat(MyArray(IndexGrammer))), What)
                    Exit Function
                End If
                Status = MyArray(IndexGrammer)
                'removed not used ?   Grammer = MyArray(IndexGrammer)
                FMB(Status)
                While Len(Status) > 0
                    X = Trim(Block(Status)) ' take care of blocks first
                    If X <> "" Then
                        Status = Status.Replace(X, " ")
                    Else
                        X = Trim(Pop(Status, MyUniverse.SysGen.Constants.ConstantDelimeters))
                    End If
                    FMB(X)
                    If X = "" Then
                    ElseIf X = "::=" Then     'syntax BNF uses ::= or :
                    ElseIf X = "'" Then
                    ElseIf X = "|" Then     ' BNF or
                    ElseIf X = ";" Then     ' BNF end of line
                        Status = "" 'ignore the rest of the line
                    ElseIf X = ")" Then     ' start group
                    ElseIf X = ")?" Then     ' start group
                    ElseIf X = ")*" Then     ' start group
                    ElseIf X = ")+" Then     ' start group
                    ElseIf X = ")-" Then     ' start group
                    ElseIf X = "(" Then     ' end group
                    ElseIf IsThisABlock(X) = True Then
                        'todo need to make sure this block is valid
                        GrammerKeyWord = Trim(WhatIsThisBlock(X))
                        If IsThisA_KeyWord(GrammerKeyWord) = True Then
                        ElseIf IsThisA_Operator(GrammerKeyWord) = True Then
                        ElseIf GrammerKeyWord = "" Then
                        Else
                            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 55 Then MyTrace(2301, X & vbTab & WhatIsThisBlock(X) & vbTab & MyArray(IndexGrammer))
                            If Len(GrammerKeyWord) = 1 And Len(X) = 3 Then ' this is a character only so ignore
                            ElseIf Hex2Bin(GrammerKeyWord) > -1 Then 'this is a single hex 8 or 16 bit character
                            ElseIf UnicodeClass(GrammerKeyWord) <> "" Then
                            ElseIf FindInSortedLanguageList("Grammer", GrammerKeyWord, Language_Grammer) > 0 Then 'this is already a keyword.
                            Else

                                AddAtomsToKeywordOROperatorORFunctionList("Grammer", GrammerKeyWord, Language_Grammer)
                                ShowSorts("Grammer", ReSortLanguagekeyWord("Grammer", MyArray, IndexGrammer))
                                MSG_Ainfo1(2098, "Keyword " & GrammerKeyWord & " added from /Grammer ", X, "")
                                DisplayMyStatus("Keyword " & GrammerKeyWord & " added from /Grammer " & X)
                            End If
                        End If
                    ElseIf BlockBreak(MyArray(IndexGrammer), "<>") <> "" Then
                        If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 55 Then MyTrace(2302, BlockBreak(MyArray(IndexGrammer), "<>"))
                    ElseIf FindMyGrammerName(X) = "" Then
                        FindingMyBugsGrammer += MSG_Abug(2121, "The name of this grammer has not been found.  ", X, MyArray(IndexGrammer))
                    Else
                        If InStr("?*+-", Right(X, 1)) > 0 Then
                            If FindMyGrammerName(Mid(X, 1, Len(X) - 1)) = "" Then
                                MyTrace(2303, X & vbTab & MyArray(IndexGrammer))
                            End If
                        Else
                            'test to see if it defined as something 
                            If FindMyGrammerName(X) = "" Then 'not used and defined.
                                MyTrace(2304, X & vbTab & MyArray(IndexGrammer))
                            Else
                                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 45 Then MyTrace(2305, "Defined " & X & vbTab & Status)
                            End If
                        End If
                    End If
                End While
            Next
        End Function




