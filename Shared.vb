            MyTrace(2212, "******* " & "I" & MyBugNumber.ToString & "  MyInfo: " & CountStack() & A & "  <--->  " & B & "  <--->  " & C & vbCrLf)
        End Sub



        'This is used to count the number of times the routine is called, so should only have one at the top of every routine.
        Public Shared Sub MSG_MyTrace1(MyBugNumber As Int32, Routine As String)
            Dim Temp, BugNumber As Int32
            BugNumber = MyBugNumber
            If BugNumber < 1 Or BugNumber / 8 > UBound(MyMessageBits1) Then
                MSG_Abug(1813, "MyTrace number is out of range 1- " & (UBound(MyMessageBits1) * 8).ToString, BugNumber.ToString, BugNumber.ToString)
            End If

            Temp = MyMinMax(BugNumber, 0, UBound(MyTraceCount))
            If Temp <> BugNumber Then
                MSG_Abug(1814, " greater than array size set to " & Temp.ToString, UBound(MyTraceCount).ToString, BugNumber.ToString)
            End If
            MyTraceCount(Temp) += 1
            MyTraceRoutine(Temp) = Left(Routine, 16)
            If IsBitSet1(Temp) = False Then
                Return
            End If
            Select Case MsgBox("#" + BugNumber.ToString & ":" & CStr(Temp) + vbTab + Routine + vbCrLf + MyTraceCount(Temp).ToString, MsgBoxStyle.YesNo, "Yes to see, No To stop Seeing This message A")
                Case MsgBoxResult.Yes
                    BitSet1(BugNumber, "On")
                Case MsgBoxResult.No
                    BitSet1(BugNumber, "Off")
                Case Else
                    BitSet1(BugNumber, "Off")
            End Select

        End Sub



        'This is a copy of MSG_MyTrace2() where I am not stopping
        Public Shared Sub MSG_MyTrace3(BugNumber As Int32, Routine As String)
            Dim A, I As Int32
            MyTracing("@MSG_MyTrace3 " & BugNumber.ToString & " " & Routine & vbCrLf)
            A = BugNumber
            If A < 1 Or A / 8 > UBound(MyMessageBits1) Then
                MSG_Abug(1818, "MyTrace3 number is out of range 1- " & UBound(MyMessageBits1).ToString, BugNumber.ToString, A.ToString)
            End If
            If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 51 Then
                I = MyMinMax(A, 0, UBound(MyTraceCount))
                If I <> A Then
                    MSG_Abug(1819, "invalid bug number tracing ", A.ToString, "Set to " & I.ToString)
                End If
                MyTraceCount(I) += 1
                MyTraceRoutine(I) = Routine
                MyTrace(2213, "**** " & "T" & CStr(I) + FD + CStr(MyTraceCount(I)) + " times for : " + vbCrLf + Routine)
            Else
                If MyUniverse.MyDebug = 0 Or MyUniverse.MyDebug > 55 Then MyTrace(2214, "***** " & "T" & BugNumber.ToString + FD + "" + Routine & vbCrLf)
            End If
        End Sub
